to signup: 
    curl -X POST http://localhost:3306/api/users/auth/signup -H "Content-Type: application/json" -d '{"firstName":"anas","lastName":"jawad","alias":"testuser","email":"test@example.com","password":"Test1234"}'


to signin:
    curl -X POST http://localhost:3306/api/users/auth/signin -H "Content-Type: application/json" -d '{"alias":"testuser","password":"Test1234"}'



curl -X POST http://localhost:3306/api/users/auth/logout -b cookie.txt

curl -X POST http://localhost:3306/api/users/auth/signin -H "Content-Type: application/json" -d '{"alias":"ilias","password":"Test1234"}' -c cookie.txt

curl -X PUT http://localhost:3306/api/users/profile/ -H "Content-Type: application/json" -d '{"rating": "200"}'
